{
  "manifest_version": 2,
  "name": "Barnvakt",
  "version": "1.0",
  "description": "",
  "icons": {
    "48": "icons/logo.png",
    "96": "icons/logo@2x.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "barnvakten.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background-script.js"
    ]
  },
  "permissions": ["storage", "activeTab"],
  "browser_action": {
    "default_icon": "icons/logo.png",
    "default_popup": "/options/dist/popup.html"
  },
  "options_ui": {
    "page": "/options/dist/options.html",
    "open_in_tab": true
  }
}